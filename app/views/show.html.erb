
<h1>Hey! <%= @user.name %></h1>

<h2>Your followers are</h2>
<% @followers.each do |follower| %>
  <p><%= follower.name %></p>
<% end %>
​
<h2>You follow</h2>
<% @followees.each do |followee| %>
  <p><%= followee.name %></p>
<% end %>
​
<h2>Here's who you don't follow</h2>
<% @not_following.each do |user| %>
  <p><%= user.name %></p> <%= form_for user, url: { action: "update" } do |f| %>
    <%= f.hidden_field :name %>
    <%= f.submit "Add Friend" %>
  <% end %>
<% end %>
